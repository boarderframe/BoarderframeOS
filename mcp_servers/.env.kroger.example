# Kroger API Configuration
# Copy this file to .env and fill in your actual API credentials

# Required: Kroger Developer API Credentials
# Get these from: https://developer.kroger.com/
KROGER_CLIENT_ID=your_client_id_here
KROGER_CLIENT_SECRET=your_client_secret_here

# Optional: OAuth Redirect URI (default: http://localhost:9004/auth/callback)
KROGER_REDIRECT_URI=http://localhost:9004/auth/callback

# Optional: Development mode (allows server to start without credentials)
KROGER_DEV_MODE=true